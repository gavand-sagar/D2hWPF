<Window x:Class="D2hWPF.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:D2hWPF"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800">
    <Grid>
        <Calendar HorizontalAlignment="Left" Margin="109,77,0,0" VerticalAlignment="Top"/>
        <Slider HorizontalAlignment="Left" Margin="524,138,0,0" VerticalAlignment="Top" Width="120"/>
        <Ellipse Height="100" Stroke="Black" Width="100">
            <Ellipse.Fill>
                <SolidColorBrush Opacity="0.5" Color="Red"></SolidColorBrush>
            </Ellipse.Fill>
        </Ellipse>

    </Grid>
</Window>
